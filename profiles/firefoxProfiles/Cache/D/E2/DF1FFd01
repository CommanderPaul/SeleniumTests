  <addon id="363974">
  <name>Lightbeam for Firefox</name>
  <type id="1">Extension</type>
  <guid>jid1-F9UJ2thwoAm5gQ@jetpack</guid>
  <slug>lightbeam</slug>
  <version>1.0.2</version>
